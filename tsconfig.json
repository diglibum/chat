{
    "compilerOptions": {
        "outDir": "dist",
        "target": "ES6",
        "declaration": false,
        "module": "commonjs",
        "strictNullChecks": true,
        "sourceMap": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true
     }
} 